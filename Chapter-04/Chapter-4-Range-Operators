import UIKit

// Range Operators and Loops
// Department Store Program
// Send Discount vouchers to customers between the ages of 18 and 30

// firstNumber...lastNumber

// Closed Range Operator
let myRange = 10...20

for Range in 10...20 {
	print(Range)
}

print("")

// Half-Open Range Operator
let myRange2 = 10..<20

for Range2 in 10..<20 {
	print(Range2)
}
